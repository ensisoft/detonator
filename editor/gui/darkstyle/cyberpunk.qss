	border-top-right-radius:2px;
	background-color: #777777;
    width: 16px;
    border-width: 1px;
}
QSpinBox::up-button:hover {
	background-color: #585858;
}
QSpinBox::up-button:pressed {
	background-color: #252525;
    width: 16px;
    border-width: 1px;
}
QSpinBox::down-button {
	border-bottom-right-radius:2px;
	background-color: #777777;
    width: 16px;
    border-width: 1px;
}
QSpinBox::down-button:hover {
	background-color: #585858;
}
QSpinBox::down-button:pressed {
	background-color: #252525;
    width: 16px;
    border-width: 1px;
}
*/

QGroupBox {
    border: 0px solid;
    border-color: #666666;
	border-radius: 5px;
	margin-top: 20px;
	font-weight: bold;
	color: #e0e0e0;
}
QGroupBox::title {
    background-color: transparent;
    subcontrol-origin: margin;
    padding: 5px;
	border-top-left-radius: 3px;
	border-top-right-radius: 3px;
}
QHeaderView::section {
    background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgba(60,60,60,255), stop:1 rgba(50,50,50,255));
	border: 1px solid #111;
    color: #e0e0e0;
    text-align: left;
	padding: 4px;
}
QHeaderView::section:disabled {
    background-color: #525251;
    color: #656565;
}
QHeaderView::section:checked {
    background-color: #333;
    color: #e0e0e0;
}

QListView,
QTableView {
	background-color: #232323;
	border: 1px solid #343434;
}


QListView::disabled {
	background-color: #656565;
	color: #1b1b1b;
    border: 1px solid #656565;
}
QListView::item {
	background-color: #2d2d2d;
    padding: 1px;
}
QListView::item:alternate {
    background-color: #3a3a3a;
}
QListView::item:selected {
	background-color: #00c8ff;
	border: 1px solid #00c8ff;
	color: #000;
}
QListView::item:hover {
    background-color: #262626;
    border: none;
    color: #fff;
}

QCheckBox {
	background-color: transparent;
    color: #e0e0e0;
	border: none;
}

QGroupBox::indicator,
QCheckBox::indicator {
    background-color: #323232;
    border: 1px solid darkgray;
    width: 12px;
    height: 12px;
}
QGroupBox::indicator:checked,
QCheckBox::indicator:checked {
	background-color: #00c8ff;
    border: 1px solid #00c8ff;
}
QGroupBox::indicator:unchecked:hover,
QCheckBox::indicator:unchecked:hover {
	border: 1px solid #00c8ff;
}
QCheckBox::disabled {
	color: #656565;
}

QRadioButton {
	color: #e0e0e0;
	background-color: transparent;
}
QRadioButton::indicator::unchecked:hover {
	border: 2px solid #00c8ff;
	border-radius: 6px;
}
QRadioButton::indicator::checked {
	border: 2px solid #00c8ff;
	border-radius: 6px;
	background-color: rgba(0,200,255,20%);
	width: 9px;
	height: 9px;
}

QSlider::groove:horizontal {
	background-color: transparent;
	height: 3px;
}
QSlider::sub-page:horizontal {
	background-color: #00c8ff;
}
QSlider::add-page:horizontal {
	background-color: #131313;
}
QSlider::handle:horizontal {
	background-color: #00c8ff;
	width: 14px;
	margin-top: -6px;
	margin-bottom: -6px;
	border-radius: 6px;
}
QSlider::handle:horizontal:hover {
	background-color: #33d9ff;
}

QScrollBar:horizontal {
	border: 1px solid #222;
	background: qlineargradient(x1:0,y1:0,x2:0,y2:1, stop:0 #121212, stop:0.2 #282828, stop:1 #484848);
	height: 7px;
	margin: 0 16px;
}
QScrollBar::handle:horizontal {
	 background: #00c8ff;
	 min-height: 20px;
	 border-radius: 2px;
}
QScrollBar::add-line:horizontal, QScrollBar::sub-line:horizontal {
	 border: 1px solid #111;
	 border-radius: 2px;
	 background: #00c8ff;
	 width: 14px;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal {
	 background: none;
}

QScrollBar:vertical {
	 background: qlineargradient(x1:0,y1:0,x2:1,y2:0, stop:0 #121212, stop:0.2 #282828, stop:1 #484848);
	 width: 7px;
	 margin: 16px 0;
	 border: 1px solid #222;
}
QScrollBar::handle:vertical {
	 background: #00c8ff;
	 min-height: 20px;
	 border-radius: 2px;
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
	 border: 1px solid #111;
	 border-radius: 2px;
	 background: #00c8ff;
	 height: 14px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
	 background: none;
}

QProgressBar {
    border: 1px solid #666;
    text-align: center;
	font-weight: bold;
	color: #e0e0e0;
}
QProgressBar::chunk {
    background-color: #00c8ff;
    width: 30px;
    margin: 0.5px;
}

)CPP_RAW_STRING"
