R"CPP_RAW_STRING(

QToolTip {
	border: 1px solid #111;
	background-color: #00c8ff;
	padding: 1px;
	border-radius: 3px;
	opacity: 100;
}

QWidget {
	background-color: #1e1e1e;
	color: #e0e0e0;
	selection-background-color: #00c8ff;
	selection-color: #000;
}

QWidget:disabled {
	color: #555;
}

QDockWidget {
	font-weight: bold;
	color: #00c8ff;
}

QLabel {
	background-color: transparent;
	color: #e0e0e0;
}
QMenuBar {
	background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgba(40,40,40,255), stop:1 rgba(30,30,30,255));
	border: 1px solid #111;
	color: #e0e0e0;
}
QMenuBar::item {
	background-color: transparent;
}
QMenuBar::item:selected {
	background-color: rgba(0,200,255,20%);
	border: 1px solid #00c8ff;
	color: #fff;
}
QMenuBar::item:pressed {
	background-color: #00c8ff;
	border: 1px solid #00c8ff;
	color: #000;
}
QMenu {
    background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgba(40,40,40,255), stop:1 rgba(30,30,30,255));
    border: 1px solid #222;
    padding: 4px;
	color: #e0e0e0;
}
QMenuBar:disabled {
	color: #555;
}

QMenu::item {
    background-color: transparent;
	padding: 2px 20px 2px 20px;
	border: 1px solid transparent;
}
QMenu::separator {
   	background-color: #444;
	height: 1px;
}
QMenu::item:disabled {
    color: #555;
    background-color: transparent;
    padding: 2px 20px 2px 20px;
}
QMenu::item:selected {
	background-color: rgba(0,200,255,20%);
	border: 1px solid #00c8ff;
	color: #fff;
}
QToolBar {
	background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgba(50,50,50,255), stop:1 rgba(35,35,35,255));
	border: 1px solid #333;
}
QToolBar::separator {
	background-color: #2e2e2e;
	width: 1px;
}
QToolButton {
	/* need this otherwise the button fidgets when hovered */
	border: 1px solid transparent;
}
QToolButton[popupMode="1"] { /* only for MenuButtonPopup */
    padding-right: 20px; /* make way for the popup button */
}

QToolButton:disabled {
	color: #555;
}
QToolButton:hover {
	background-color: rgba(0,200,255,20%);
	border: 1px solid #00c8ff;
}
QToolButton:pressed {
	background-color: #2b2b2b;
}
QToolButton:checked {
	background-color: #2b2b2b;
}
QPushButton {
	color: #e0e0e0;
	border-style: solid;
	border-width: 1px;
	border-radius: 3px;
	border-color: #00c8ff;
	padding: 5px;
}
QPushButton::flat {
	background-color: transparent;
	border: none;
	color: #e0e0e0;
}
QPushButton::disabled {
	background-color: #2a2a2a;
	color: #656565;
	border-color: #333;
}
QPushButton::hover {
	background-color: rgba(0,200,255,20%);
	border: 1px solid #00c8ff;
}
QPushButton::pressed {
	background-color: #2e2e2e;
	border: 1px solid #00c8ff;
}
QPushButton::checked {
	background-color: #00c8ff;
	border: 1px solid #00c8ff;
	color: #000;
}
QLineEdit {
	background-color: #232323;
	color : #e0e0e0;
	border: 1px solid #343434;
	border-radius: 2px;
	padding: 3px;
	padding-left: 5px;
}
QPlainTextEdit {
	background-color: #232323;
	color : #e0e0e0;
	border: 1px solid #343434;
	border-radius: 2px;
	padding: 3px;
	padding-left: 5px;
}
QLineEdit:focus, QPlainTextEdit:focus {
	border: 1px solid #00c8ff;
}

QTabWidget {
	border-style: none;
}

QTabBar::tab {
	background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgba(84,84,84,255), stop:1 rgba(59,59,59,255));
	color: #e0e0e0;
	border-style: solid;
	border-width: 1px;
	border-color: #666;
	padding: 1px 15px;
}
QTabBar::tab:selected {
	background-color: rgba(0,200,255,20%);
	border: 1px solid #00c8ff;
}
QTabBar::tab:!selected:hover {
	border-color: #00c8ff;
}


QComboBox {
    background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgba(84,84,84,255), stop:1 rgba(59,59,59,255));
    border: 1px solid #000;
    padding-left: 6px;
    color: #e0e0e0;
    height: 25px;
}
QComboBox::disabled {
	background-color: #404040;
	color: #656565;
	border-color: #333;
}
QComboBox:on {
    background-color: #00c8ff;
	color: #000;
}
QComboBox QAbstractItemView {
    background-color: #383838;
    color: #e0e0e0;
    border: 1px solid #111;
    selection-background-color: #00c8ff;
    outline: 0;
}
QComboBox::drop-down {
	background-color: qlineargradient(spread:repeat, x1:1, y1:0, x2:1, y2:1, stop:0 rgba(57,57,57,255), stop:1 rgba(50,50,50,255));
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 15px;
    border-left: 1px solid #111;
}
QComboBox::down-arrow {
	image: url(":/combinear/icons/down_arrow.png");
}


QSpinBox,
QDoubleSpinBox {
    background-color: #232323;
	color : #e0e0e0;
	border: 1px solid #343434;
	padding: 3px 5px;
    border-radius : 2px;
}

QSpinBox::up-button,
QDoubleSpinBox::up-button {
	image: url(":combinear/icons/up_arrow.png");
}

QSpinBox::down-button,
QDoubleSpinBox::down-button {
	image: url(":combinear/icons/down_arrow.png");
}

